<div class="panel panel-default">
    <div class="panel-heading">
        {{vm.params.channelName + '日明细'}}
    </div>
    <div class="panel-body">
        <div class="table-responsive border">

        <table class="table" style="width: 1800px">
            <thead>
            <tr class="grey">
                <th>序号</th>
                <th>日期</th>
                <th>注册量</th>
                <th>充值人数</th>
                <th>充值金额</th>
                <th>首次充值人数</th>
                <th>首次充值总额</th>
                <th>购彩人数</th>
                <th>购彩金额</th>
                <th>首次购彩人数</th>
                <th>首次购彩总额</th>
                <th>中奖人数</th>
                <th>中奖金额</th>
                <th>操作</th>
            </tr>

            </thead>

            <tbody>
            <tr ng-repeat="item in vm.dayList track by $index">
                <td ng-bind="$index+1"></td>
                <td ng-bind="item.createAt|date:'yyyy-MM-dd HH:mm'"></td>
                <td ng-bind="item.userCount"></td>
                <td ng-bind="item.paymentCount"></td>
                <td ng-bind="item.paymentSum"></td>
                <td ng-bind="item.firstPayCount"></td>
                <td ng-bind="item.firstPaySum"></td>
                <td ng-bind="item.buyCount"></td>
                <td ng-bind="item.buySum"></td>
                <td ng-bind="item.firstBuyCount"></td>
                <td ng-bind="item.firstBuySum"></td>
                <td ng-bind="item.awardCount"></td>
                <td ng-bind="item.awardSum"></td>
                <td>
                    <button class="btn btn-primary  btn-sm" ui-sref="field.shoppingList({channelNo:item.channelNo,createAt:item.createAt})">购彩明细</button>
                </td>
            </tr>
            </tbody>
        </table>
        <pagination ng-if="vm.params.total" total="{{vm.params.total}}"></pagination>
    </div>
</div>        </div>
