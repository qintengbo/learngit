/* 搜索导航 */
.findJob_page {
  padding-top: 20px;
  background: #f6f7f8;
}
.findJob_header {
  font-size: 0;
  background: #fff;
}
.findJob_nav {
  position: relative;
  display: flex;
  flex-direction: column;
  float: left;
  width: 21%;
  height: 460px;
  font-size: 1.4rem;
  background: #f43851;
}
@media screen and (max-width: 991px) {
  .findJob_nav {
    display: none;
  }
}
.findJob_submenu {
  display: inline-block;
  margin-right: 10px;
  padding: 7px 12px;
  cursor: pointer;
}
.findJob_menu_hover {
  position: absolute;
  width: 700px;
  top: 0;
  left: 100%;
  z-index: 99;
  background: #fff;
  min-height: 460px;
  padding: 20px 0 30px 40px;
  border-top: 1px solid #f43851;
  border-right: 1px solid #f43851;
  border-bottom: 1px solid #f43851;
}
.findJob_menu_hover:hover {
  display: inline-block;
}
.findJob_menu {
  padding: 8px 25px 8px 15px;
  position: static !important;
  flex: 1;
  color: #fff;
  border-bottom: solid 1px #f43851;
  border-top: solid 1px #f43851;
  border-left: solid 1px #f43851;
}
.findJob_menu:hover {
  color: #f43851;
  background: #fff;
}
.findJob_menu:hover .disp1 {
  display: none;
}
.findJob_menu:hover .disp2 {
  display: inline-block;
}
